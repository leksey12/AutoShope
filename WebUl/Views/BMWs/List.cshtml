@using Domain.AutoShop
@model IEnumerable<BMW>

@{
    ViewBag.Title = "BMW";
}
<div>
    <h3>Изар-Авто:</h3>
    <table>
        <tr>
            <th>
                Название
            </th>

            <th>
                Модель
            </th>

            <th>
                цена
            </th>

        </tr>
        @foreach (BMW p in Model)
            {
            <tr>
                <td><p>@p.Name </p></td>
                <td><p>@p.Model</p></td>
                <td><p>@p.Price.ToString("# руб")
            </tr>
        }
    </table>
    <a href="@Url.Action("Index","Home")"><h4>Назад</h4></a>
</div>


@using Domain.AutoShop
@model IEnumerable<Lada>

@{
    ViewBag.Title = "Lada";
}
<div>
    <h3>ВИКИНГИ:</h3>
    <table>
        <tr>
            <th>
                Название
            </th>

            <th>
                Модель
            </th>

            <th>
                цена
            </th>

        </tr>
        @foreach (Lada p in Model)
            {
            <tr>
                <td><p>@p.Name </p></td>
                <td><p>@p.Model</p></td>
                <td><p>@p.Price.ToString("# руб")</p></td>
            </tr>
        }
    </table>
    <a href="@Url.Action("Index","Home")"><h4>Назад</h4></a>
</div>

